{
    "name": "afb-display",
    "port": 1234,
    "alias": [
        "/devtools:/usr/share/afb-ui-devtools/binder"
    ],
    "monitoring": true,
    "ws-client": [
        "tcp:phytec-power.tuxevse.vpn:12351/engy"
    ],
    "uuid": "HELLO",
    "binding": [
        {
            "uid": "display-lvgl",
            "api": "display",
            "path": "${CARGO_TARGET_DIR}debug/libafb_display_lvgl.so",
            "info": "lvgl embedded display binding",
            "permission": "acl:display:client",
            "test": 1,
            "display": {
                "x_res": 1024,
                "y_res": 600,
                "ratio": 1
            },
            "engy_api": "engy"
        }
    ]
}
